{
  "name": "baseimage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"docker compose up\"",
    "build": "SKIP_ENV_VALIDATION=true next build",
    "start": "next start",
    "lint": "eslint",
    "db:push": "dotenv drizzle-kit push",
    "db:migrate": "dotenv drizzle-kit migrate",
    "db:up": "dotenv drizzle-kit up",
    "db:generate-migrations": "dotenv drizzle-kit generate",
    "db:validate-migrations": "pnpm db:generate-migrations | tee migrations.log | grep -q \"nothing to migrate\" || exit 1",
    "db:studio": "dotenv drizzle-kit studio",
    "type-check": "tsc --noEmit",
    "knip": "SKIP_ENV_VALIDATION=true knip",
    "test": "vitest"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@t3-oss/env-nextjs": "^0.13.10",
    "ai": "^6.0.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-safe-action": "^8.0.11",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "usehooks-ts": "^3.1.1",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.1",
    "jsdom": "^27.4.0",
    "knip": "^5.79.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
